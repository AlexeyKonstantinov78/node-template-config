# Editor configuration, see http://editorconfig.org
root = true

[*]
end_of_line = lf
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[{*.js, *.cjs, *.mjs, *.jsx, *.ts, *.tsx, *.json, *yml}]
max_line_length = off
trim_trailing_whitespace = false
